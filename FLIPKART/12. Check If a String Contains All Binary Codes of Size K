

class Solution {
public:
    bool hasAllCodes(string s, int k) {
        unordered_map<string,int> mp;
        if(s.size()<k)return false;
        for(int i=0;i<=s.size()-k;i++){
                string str=s.substr(i,k);
                mp[str]++;
        }
        return mp.size()==pow(2,k);
        
    }
};
